{
    "manifest_version": 2,
    "name": "Sugify",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_title": "Sugify"
    },
    "icons": {
        "128": "icon.png"
    },
    "background": {
        "scripts": ["background.js"]
    }
}
